#!/bin/bash

qsub -v U='0',L='20' -N L20U0 run_time_ev_test
qsub -v U='1',L='20' -N L20U1 run_time_ev_test
qsub -v U='4',L='20' -N L20U4 run_time_ev_test
qsub -v U='8',L='20' -N L20U8 run_time_ev_test

qsub -v U='0',L='40' -N L40U0 run_time_ev_test
qsub -v U='1',L='40' -N L40U1 run_time_ev_test
qsub -v U='4',L='40' -N L40U4 run_time_ev_test
qsub -v U='8',L='40' -N L40U8 run_time_ev_test

qsub -v U='0',L='80' -N L80U0 run_time_ev_test
qsub -v U='1',L='80' -N L80U1 run_time_ev_test
qsub -v U='4',L='80' -N L80U4 run_time_ev_test
qsub -v U='8',L='80' -N L80U8 run_time_ev_test



