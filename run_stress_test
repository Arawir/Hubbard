#PBS -S /bin/bash
#PBS -q main
#PBS -l walltime=00:01:00
#PBS -l select=1:ncpus=1:mem=36GB

cd $PBS_O_WORKDIR

echo "cutoff: ${cutoff}";
echo "L: ${L}";
./Hubbard t=1.0 U=1.0 L=${L} cutoff=${cutoff} exp=DMRG state=Up-Dn Silent=0 maxDim=2048  niter=10 ConserveQNs=1 ConserveNf=1 ConserveSz=1 > StressL${L}E${cutoff:3:1}.data

qstat -f $PBS_JOBID | grep resources_used.vmem
qstat -f $PBS_JOBID | grep resources_used.walltime
